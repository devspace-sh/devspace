  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^10.17.60",
    "@types/react": "^16.14.65",
    "@types/react-dom": "^16.9.25",
